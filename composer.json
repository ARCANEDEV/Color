{
    "name":        "arcanedev/color",
    "description": "PHP Helper for manipulating colors.",
    "keywords": [
        "arcanedev", "color", "hex", "rgb"
    ],
    "homepage": "https://github.com/ARCANEDEV/Color",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":             ">=5.6"
    },
    "require-dev": {
        "phpunit/phpcov":  "~2.0|~3.0",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\Color\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\Color\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.0\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
