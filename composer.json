{
    "name":        "arcanedev/color",
    "description": "PHP Helper for manipulating colors (Laravel 5.x supported).",
    "keywords": [
        "arcanedev", "color", "hex", "rgb", "rgba", "hsl", "hsv", "converter", "validation", "laravel"
    ],
    "homepage": "https://github.com/ARCANEDEV/Color",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":               ">=7.0",
        "arcanedev/support": "~4.2"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5.0",
        "phpunit/phpcov":      "~4.0",
        "phpunit/phpunit":     "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\Color\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
